@model Ranking_Estudiantil.Models.Projects

@{
    ViewData["Title"] = "Delete";
}





<body style="background-color: #232323;">
    <div align="center">
        <h1 style="color:white;">Eliminar</h1>

        <h4 style="color:white;">Proyecto</h4>
    </div>
    <br>
    <div align="center">
         <div class="col-md-4">
        <form asp-action="Delete">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="ProjectsID" />
            
            <div class="form-group">
                <label asp-for="achievment" class="control-label"></label>
                <input asp-for="achievment" class="form-control" />
                <span asp-validation-for="achievment" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ProjectName" class="control-label"></label>
                <input asp-for="ProjectName" class="form-control" />
               
            </div>
            <div class="form-group">
                <label asp-for="punctuation" class="control-label"></label>
                <input asp-for="punctuation" class="form-control" />
                <span asp-validation-for="punctuation" class="text-danger"></span>
            </div>
            
            <div class="form-group">
                <input type="submit" value="Eliminar" class="btn btn-danger" />
            </div>
        </form>
    </div>
        <div>
            <a asp-controller="Students" asp-action="Index">Volver a la lista</a>
        </div>
    </div>
</body>
