@model IEnumerable<Ranking_Estudiantil.Models.Student>

@{
    ViewData["Title"] = "Index";
}

<h1>Ranking</h1>
<table class="table">
    <thead>
        <tr>
           
            <th>
                @Html.DisplayNameFor(model => model.Score)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
           
            <td>
                @Html.DisplayFor(modelItem => item.Score)
            </td>
            <td>
                    
                <img src="~/Images/DiamondR.png"/>
            </td>
        </tr>
}
    </tbody>
</table>
